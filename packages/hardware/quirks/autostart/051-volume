. /storage/.config/profile.d/001-device_config

# start virtual audio driver

VAUDIODRV=$(get_setting audio.v-driver)

if [ -z "${VAUDIODRV}" ]
then
  /usr/bin/audio_vdriver.sh --start
else
  /usr/bin/audio_vdriver.sh ${VAUDIODRV}
fi
